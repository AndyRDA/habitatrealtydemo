{
  "$ref": "#/definitions/rentals",
  "definitions": {
    "rentals": {
      "type": "object",
      "properties": {
        "projectName": {
          "type": "string"
        },
        "listingNumber": {
          "type": "string",
          "format": "uuid"
        },
        "price": {
          "type": "number"
        },
        "address": {
          "type": "string"
        },
        "location": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "isAddressShown": {
          "type": "boolean"
        },
        "pubDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "integer",
              "format": "unix-time"
            }
          ]
        },
        "agent": {
          "type": "string"
        },
        "occupationDate": {
          "anyOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "integer",
              "format": "unix-time"
            }
          ]
        },
        "leasePeriod": {
          "type": "string",
          "enum": [
            "1 Month",
            "6 Months",
            "12 Months",
            "24 Months"
          ]
        },
        "depositRequirements": {
          "type": "string"
        },
        "levies": {
          "type": "number"
        },
        "propertyType": {
          "type": "string",
          "enum": [
            "House",
            "Town House",
            "Apartment / Flat",
            "Commercial",
            "Industrial",
            "Farm",
            "Land"
          ]
        },
        "bedrooms": {
          "type": "number"
        },
        "bathrooms": {
          "type": "number"
        },
        "garages": {
          "type": "number"
        },
        "parking": {
          "type": "number"
        },
        "erfSize": {
          "type": "number"
        },
        "floorSize": {
          "type": "number"
        },
        "petsAllowed": {
          "type": "boolean"
        },
        "furnishedStatus": {
          "type": "string",
          "enum": [
            "Furnished",
            "Unfurnished"
          ]
        },
        "hasPool": {
          "type": "boolean"
        },
        "hasGarden": {
          "type": "boolean"
        },
        "hasFibre": {
          "type": "boolean"
        },
        "hasSolar": {
          "type": "boolean"
        },
        "hasBackupPower": {
          "type": "boolean"
        },
        "descTitle": {
          "type": "string"
        },
        "mainImage": {
          "type": "string"
        },
        "aboutImages": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "$schema": {
          "type": "string"
        }
      },
      "required": [
        "projectName",
        "listingNumber",
        "price",
        "address",
        "location",
        "city",
        "isAddressShown",
        "pubDate",
        "agent",
        "mainImage"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}